<?xml version='1.0' encoding='utf-8'?>
<widget id="miwebapp.cursonode.com" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hola Aplicacion</name>
    <description>
        Descripcion hola aplicacion
    </description>
    <author email="curso.mean@gmail.com" href="http://cordova.io">
        Node Mean
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
